NODE events_by_min
SQL >

    SELECT
        toStartOfMinute(datetime) AS ts,
        product,
        countState() AS n_events,
        countIfState(event = 'view') AS n_views,
        countIfState(event = 'cart') AS n_carts,
        countIfState(event = 'sale') AS n_sales
    FROM web_events
    GROUP BY
        ts,
        product

TYPE materialized
DATASOURCE events_by_min_mv


